{% assign ai_gen_id = block.id | replace: '_', '' | downcase %}

{% style %}
  .ai-instagram-grid-{{ ai_gen_id }} {
    padding: {{ block.settings.section_padding_top }}px {{ block.settings.section_padding_horizontal }}px {{ block.settings.section_padding_bottom }}px;
    background-color: {{ block.settings.background_color }};
  }

  .ai-instagram-grid__container-{{ ai_gen_id }} {
    max-width: {{ block.settings.max_width }}px;
    margin: 0 auto;
  }

  .ai-instagram-grid__header-{{ ai_gen_id }} {
    text-align: {{ block.settings.text_alignment }};
    margin-bottom: {{ block.settings.header_spacing }}px;
  }

  .ai-instagram-grid__title-{{ ai_gen_id }} {
    font-family: {{ block.settings.title_font.family }}, {{ block.settings.title_font.fallback_families }};
    font-weight: {{ block.settings.title_font.weight }};
    font-style: {{ block.settings.title_font.style }};
    font-size: {{ block.settings.title_size }}px;
    color: {{ block.settings.title_color }};
    margin: 0 0 {{ block.settings.title_spacing }}px;
    text-transform: {{ block.settings.title_transform }};
    letter-spacing: {{ block.settings.title_letter_spacing }}px;
  }

  .ai-instagram-grid__subtitle-wrapper-{{ ai_gen_id }} {
    display: inline-flex;
    justify-content: center;
    align-items: center;
    margin: 0 auto;
  }

  .ai-instagram-grid__subtitle-button-{{ ai_gen_id }} {
    cursor: pointer;
    width: 17.5em;
    height: 5em;
    border-radius: 1.5em;
    border: none;
    display: flex;
    justify-content: right;
    align-items: center;
    box-shadow: 0em 0em 0.25em 0em rgba(0, 0, 0, 0.25);
    position: relative;
    overflow: hidden;
    background-color: #fafafa;
    transition: all 0.25s ease-in-out;
  }

  .ai-instagram-grid__subtitle-text-{{ ai_gen_id }} {
    font-family: {{ block.settings.subtitle_font.family }}, {{ block.settings.subtitle_font.fallback_families }};
    font-weight: {{ block.settings.subtitle_font.weight }};
    font-style: {{ block.settings.subtitle_font.style }};
    font-size: {{ block.settings.subtitle_size }}px;
    color: {{ block.settings.subtitle_color }};
    margin: 0;
    position: absolute;
    top: 50%;
    left: 0.75em;
    transform: translateY(-50%);
    transition: all 0.5s ease-in-out;
  }

  .ai-instagram-grid__subtitle-glow-{{ ai_gen_id }} {
    width: 4em;
    height: 3.5em;
    background-color: #77ff9f;
    border-radius: 1em;
    position: relative;
    box-shadow: 0em 0em 0.5em 0em rgba(0, 0, 0, 0.25), inset 0.25em 0em 0.25em rgb(195, 255, 0);
    overflow: hidden;
    margin-right: 0.75em;
    transition: all 0.5s ease-in-out;
  }

  .ai-instagram-grid__subtitle-icon-{{ ai_gen_id }} {
    position: absolute;
    top: 50%;
    left: 50%;
    transform: translate(-50%, -50%);
    z-index: 2;
    width: 30px;
    height: 30px;
  }

  .ai-instagram-grid__subtitle-white-{{ ai_gen_id }} {
    width: 100%;
    background-color: rgba(255, 255, 255, 0.5);
    height: 2.5em;
    position: absolute;
    top: 0em;
    filter: blur(0.5em);
  }

  .ai-instagram-grid__subtitle-red-{{ ai_gen_id }} {
    position: absolute;
    top: 50%;
    left: 50%;
    transform: translate(-50%, -50%) scale(4);
    animation: rotar-{{ ai_gen_id }} 5s linear infinite;
    filter: blur(0.2em);
    transition: all 0.5s ease-in-out;
  }

  .ai-instagram-grid__subtitle-blue-{{ ai_gen_id }} {
    position: absolute;
    top: 50%;
    left: 50%;
    transform: translate(-5%, -5%) scale(2);
    animation: rotar-{{ ai_gen_id }} 5s linear infinite;
    filter: blur(0.4em);
    transition: all 0.5s ease-in-out;
  }

  .ai-instagram-grid__subtitle-cian-{{ ai_gen_id }} {
    position: absolute;
    top: 50%;
    left: 50%;
    transform: translate(-75%, -5%) scale(2);
    animation: rotar-{{ ai_gen_id }} 5s linear infinite;
    filter: blur(0.25em);
    transition: all 0.5s ease-in-out;
  }

  .ai-instagram-grid__subtitle-yellow-{{ ai_gen_id }} {
    position: absolute;
    top: 50%;
    left: 50%;
    transform: translate(-95%, -55%) scale(2.5);
    animation: rotar-{{ ai_gen_id }} 5s linear infinite;
    filter: blur(0.25em);
    transition: all 0.5s ease-in-out;
  }

  @keyframes rotar-{{ ai_gen_id }} {
    0% {
      rotate: 0deg;
    }
    100% {
      rotate: 360deg;
    }
  }

  .ai-instagram-grid__subtitle-button-{{ ai_gen_id }}:hover .ai-instagram-grid__subtitle-text-{{ ai_gen_id }} {
    left: -25%;
    color: white;
  }

  .ai-instagram-grid__subtitle-button-{{ ai_gen_id }}:hover .ai-instagram-grid__subtitle-glow-{{ ai_gen_id }} {
    width: 16em;
    box-shadow: 0em 0em 0.5em 0em rgba(0, 0, 0, 0.25), inset 0.5em 0em 0.5em rgb(195, 255, 0);
  }

  .ai-instagram-grid__subtitle-button-{{ ai_gen_id }}:hover .ai-instagram-grid__subtitle-red-{{ ai_gen_id }},
  .ai-instagram-grid__subtitle-button-{{ ai_gen_id }}:hover .ai-instagram-grid__subtitle-blue-{{ ai_gen_id }},
  .ai-instagram-grid__subtitle-button-{{ ai_gen_id }}:hover .ai-instagram-grid__subtitle-cian-{{ ai_gen_id }},
  .ai-instagram-grid__subtitle-button-{{ ai_gen_id }}:hover .ai-instagram-grid__subtitle-yellow-{{ ai_gen_id }} {
    filter: blur(0.75em);
    top: 50%;
    left: 50%;
  }

  .ai-instagram-grid__subtitle-button-{{ ai_gen_id }}:active {
    transform: scale(0.95);
  }

  .ai-instagram-grid__subtitle-button-{{ ai_gen_id }}:active .ai-instagram-grid__subtitle-glow-{{ ai_gen_id }} {
    opacity: 0.75;
    filter: brightness(0.75);
  }

  .ai-instagram-grid__grid-{{ ai_gen_id }} {
    display: grid;
    grid-template-columns: repeat({{ block.settings.columns_desktop }}, 1fr);
    gap: {{ block.settings.grid_gap }}px;
    margin-top: {{ block.settings.grid_spacing }}px;
    padding: 0 {{ block.settings.desktop_grid_side_gap }}px;
  }

  .ai-instagram-grid__item-{{ ai_gen_id }} {
    position: relative;
    aspect-ratio: 4 / 5;
    overflow: hidden;
    border-radius: {{ block.settings.item_border_radius }}px;
    background-color: {{ block.settings.item_background }};
  }

  .ai-instagram-grid__media-{{ ai_gen_id }} {
    width: 100%;
    height: 100%;
    object-fit: cover;
    display: block;
  }

  .ai-instagram-grid__placeholder-{{ ai_gen_id }} {
    width: 100%;
    height: 100%;
    display: flex;
    align-items: center;
    justify-content: center;
    background-color: {{ block.settings.placeholder_background }};
  }

  .ai-instagram-grid__placeholder-{{ ai_gen_id }} svg {
    width: 60%;
    height: 60%;
    opacity: 0.3;
  }

  .ai-instagram-grid__overlay-{{ ai_gen_id }} {
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    background-color: {{ block.settings.overlay_color }};
    opacity: 0;
    transition: opacity 0.3s ease;
    display: flex;
    align-items: center;
    justify-content: center;
  }

  .ai-instagram-grid__item-{{ ai_gen_id }}:hover .ai-instagram-grid__overlay-{{ ai_gen_id }} {
    opacity: {{ block.settings.overlay_opacity | divided_by: 100.0 }};
  }

  .ai-instagram-grid__overlay-icon-{{ ai_gen_id }} {
    width: {{ block.settings.overlay_icon_size }}px;
    height: {{ block.settings.overlay_icon_size }}px;
    color: {{ block.settings.overlay_icon_color }};
  }

  @media screen and (max-width: 749px) {
    .ai-instagram-grid-{{ ai_gen_id }} {
      padding: {{ block.settings.section_padding_top_mobile }}px {{ block.settings.section_padding_horizontal_mobile }}px {{ block.settings.section_padding_bottom_mobile }}px;
    }

    .ai-instagram-grid__subtitle-button-{{ ai_gen_id }} {
      width: 14em;
      height: 4em;
    }

    .ai-instagram-grid__subtitle-text-{{ ai_gen_id }} {
      font-size: {{ block.settings.subtitle_size_mobile }}px;
    }

    .ai-instagram-grid__grid-{{ ai_gen_id }} {
      grid-template-columns: repeat({{ block.settings.columns_mobile }}, 1fr);
      gap: {{ block.settings.grid_gap_mobile }}px;
      padding: 0 {{ block.settings.mobile_grid_side_gap }}px;
    }

    .ai-instagram-grid__title-{{ ai_gen_id }} {
      font-size: {{ block.settings.title_size_mobile }}px;
    }
  }
{% endstyle %}

<div class="ai-instagram-grid-{{ ai_gen_id }}" {{ block.shopify_attributes }}>
  <div class="ai-instagram-grid__container-{{ ai_gen_id }}">
    {% if block.settings.show_header %}
      <div class="ai-instagram-grid__header-{{ ai_gen_id }}">
        {% if block.settings.title != blank %}
          <h2 class="ai-instagram-grid__title-{{ ai_gen_id }}">{{ block.settings.title }}</h2>
        {% endif %}
        {% if block.settings.subtitle != blank %}
          <div class="ai-instagram-grid__subtitle-wrapper-{{ ai_gen_id }}">
            <div class="ai-instagram-grid__subtitle-button-{{ ai_gen_id }}">
              <p class="ai-instagram-grid__subtitle-text-{{ ai_gen_id }}">{{ block.settings.subtitle }}</p>
              <div class="ai-instagram-grid__subtitle-glow-{{ ai_gen_id }}">
                <svg
                  class="ai-instagram-grid__subtitle-icon-{{ ai_gen_id }}"
                  xmlns="http://www.w3.org/2000/svg"
                  viewBox="0 0 24 24"
                  fill="none"
                  stroke="#fafafa"
                  stroke-width="2"
                  stroke-linecap="round"
                  stroke-linejoin="round"
                >
                  <rect x="2" y="2" width="20" height="20" rx="5" ry="5"></rect>
                  <path d="M16 11.37A4 4 0 1 1 12.63 8 4 4 0 0 1 16 11.37z"></path>
                  <line x1="17.5" y1="6.5" x2="17.51" y2="6.5"></line>
                </svg>
                <svg
                  class="ai-instagram-grid__subtitle-red-{{ ai_gen_id }}"
                  viewBox="0 0 200 200"
                  xmlns="http://www.w3.org/2000/svg"
                >
                  <path
                    fill="#ff8b80"
                    d="M50.1,-30.7C54.6,-21.2,40.8,-2.8,29.4,11.7C18,26.1,9,36.7,0.5,36.4C-8,36.1,-15.9,24.9,-26.5,10.9C-37.1,-3.1,-50.4,-19.9,-46.5,-29.1C-42.5,-38.3,-21.2,-39.9,0.8,-40.3C22.8,-40.8,45.6,-40.1,50.1,-30.7Z"
                    transform="translate(100 100)"
                  ></path>
                </svg>
                <svg
                  class="ai-instagram-grid__subtitle-yellow-{{ ai_gen_id }}"
                  viewBox="0 0 200 200"
                  xmlns="http://www.w3.org/2000/svg"
                >
                  <path
                    fill="#F8D32C"
                    d="M42.3,-26.7C50.5,-10.4,49.6,8.9,41.1,23.3C32.5,37.7,16.3,47.3,-2,48.4C-20.2,49.5,-40.3,42.2,-45.8,29.6C-51.2,17,-41.9,-0.9,-31.8,-18.4C-21.8,-35.9,-10.9,-52.9,3.1,-54.7C17.1,-56.5,34.2,-43.1,42.3,-26.7Z"
                    transform="translate(100 100)"
                  ></path>
                </svg>
                <svg
                  class="ai-instagram-grid__subtitle-cian-{{ ai_gen_id }}"
                  viewBox="0 0 200 200"
                  xmlns="http://www.w3.org/2000/svg"
                >
                  <path
                    fill="#5CFBF8"
                    d="M33.1,-23.8C40.7,-5.9,43.3,10.3,36.9,30.2C30.6,50.1,15.3,73.7,0.7,73.3C-14,72.9,-28,48.6,-36.1,27.7C-44.2,6.8,-46.4,-10.7,-39.4,-28.2C-32.5,-45.8,-16.2,-63.4,-1.8,-62.4C12.7,-61.4,25.4,-41.7,33.1,-23.8Z"
                    transform="translate(100 100)"
                  ></path>
                </svg>
                <svg
                  class="ai-instagram-grid__subtitle-blue-{{ ai_gen_id }}"
                  viewBox="0 0 200 200"
                  xmlns="http://www.w3.org/2000/svg"
                >
                  <path
                    fill="#AE9BFD"
                    d="M37.9,-16.5C44.5,-10.5,42.1,6.2,34.3,26.2C26.4,46.2,13.2,69.6,-3.7,71.7C-20.6,73.9,-41.2,54.8,-49,34.7C-56.8,14.7,-51.9,-6.2,-41.6,-14.3C-31.3,-22.5,-15.7,-17.8,0,-17.8C15.7,-17.8,31.3,-22.5,37.9,-16.5Z"
                    transform="translate(100 100)"
                  ></path>
                </svg>
                <div class="ai-instagram-grid__subtitle-white-{{ ai_gen_id }}"></div>
              </div>
            </div>
          </div>
        {% endif %}
      </div>
    {% endif %}

    <div class="ai-instagram-grid__grid-{{ ai_gen_id }}">
      {% for i in (1..12) %}
        {% liquid
          assign item_enabled_key = 'item_' | append: i | append: '_enabled'
          assign item_type_key = 'item_' | append: i | append: '_type'
          assign item_image_key = 'item_' | append: i | append: '_image'
          assign item_video_key = 'item_' | append: i | append: '_video'
          assign item_link_key = 'item_' | append: i | append: '_link'

          assign item_enabled = block.settings[item_enabled_key]
          assign item_type = block.settings[item_type_key]
          assign item_image = block.settings[item_image_key]
          assign item_video = block.settings[item_video_key]
          assign item_link = block.settings[item_link_key]
        %}

        {% if item_enabled %}
          <div class="ai-instagram-grid__item-{{ ai_gen_id }}">
            {% if item_link != blank %}
              <a href="{{ item_link }}" class="ai-instagram-grid__link-{{ ai_gen_id }}">
            {% endif %}

            {% if item_type == 'image' %}
              {% if item_image != blank %}
                <img
                  src="{{ item_image | image_url: width: 800 }}"
                  alt="{{ item_image.alt | escape }}"
                  class="ai-instagram-grid__media-{{ ai_gen_id }}"
                  loading="lazy"
                  width="800"
                  height="1000"
                >
              {% else %}
                <div class="ai-instagram-grid__placeholder-{{ ai_gen_id }}">
                  {{ 'image' | placeholder_svg_tag }}
                </div>
              {% endif %}
            {% elsif item_type == 'video' %}
              {% if item_video != blank %}
                <video
                  class="ai-instagram-grid__media-{{ ai_gen_id }}"
                  playsinline
                  {% if block.settings.video_autoplay %}autoplay{% endif %}
                  {% if block.settings.video_loop %}loop{% endif %}
                  {% if block.settings.video_muted %}muted{% endif %}
                >
                  <source src="{{ item_video }}" type="video/mp4">
                </video>
              {% else %}
                <div class="ai-instagram-grid__placeholder-{{ ai_gen_id }}">
                  {{ 'image' | placeholder_svg_tag }}
                </div>
              {% endif %}
            {% endif %}

            {% if block.settings.show_overlay %}
              <div class="ai-instagram-grid__overlay-{{ ai_gen_id }}">
                <svg class="ai-instagram-grid__overlay-icon-{{ ai_gen_id }}" xmlns="http://www.w3.org/2000/svg" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round">
                  <rect x="2" y="2" width="20" height="20" rx="5" ry="5"></rect>
                  <path d="M16 11.37A4 4 0 1 1 12.63 8 4 4 0 0 1 16 11.37z"></path>
                  <line x1="17.5" y1="6.5" x2="17.51" y2="6.5"></line>
                </svg>
              </div>
            {% endif %}

            {% if item_link != blank %}
              </a>
            {% endif %}
          </div>
        {% endif %}
      {% endfor %}
    </div>
  </div>
</div>

{% schema %}
{
  "name": "Instagram grid",
  "tag": null,
  "settings": [
    {
      "type": "header",
      "content": "Section header"
    },
    {
      "type": "checkbox",
      "id": "show_header",
      "label": "Show header",
      "default": true
    },
    {
      "type": "text",
      "id": "title",
      "label": "Title",
      "default": "Follow us on Instagram"
    },
    {
      "type": "text",
      "id": "subtitle",
      "label": "Subtitle",
      "default": "@yourhandle"
    },
    {
      "type": "select",
      "id": "text_alignment",
      "label": "Text alignment",
      "options": [
        {
          "value": "left",
          "label": "Left"
        },
        {
          "value": "center",
          "label": "Center"
        },
        {
          "value": "right",
          "label": "Right"
        }
      ],
      "default": "center"
    },
    {
      "type": "header",
      "content": "Title style"
    },
    {
      "type": "font_picker",
      "id": "title_font",
      "label": "Font",
      "default": "helvetica_n7"
    },
    {
      "type": "range",
      "id": "title_size",
      "min": 16,
      "max": 80,
      "step": 2,
      "unit": "px",
      "label": "Size (desktop)",
      "default": 40
    },
    {
      "type": "range",
      "id": "title_size_mobile",
      "min": 16,
      "max": 60,
      "step": 2,
      "unit": "px",
      "label": "Size (mobile)",
      "default": 28
    },
    {
      "type": "color",
      "id": "title_color",
      "label": "Color",
      "default": "#000000"
    },
    {
      "type": "select",
      "id": "title_transform",
      "label": "Transform",
      "options": [
        {
          "value": "none",
          "label": "None"
        },
        {
          "value": "uppercase",
          "label": "Uppercase"
        },
        {
          "value": "lowercase",
          "label": "Lowercase"
        },
        {
          "value": "capitalize",
          "label": "Capitalize"
        }
      ],
      "default": "none"
    },
    {
      "type": "range",
      "id": "title_letter_spacing",
      "min": 0,
      "max": 5,
      "step": 0.5,
      "unit": "px",
      "label": "Letter spacing",
      "default": 0
    },
    {
      "type": "range",
      "id": "title_spacing",
      "min": 0,
      "max": 40,
      "step": 2,
      "unit": "px",
      "label": "Bottom spacing",
      "default": 8
    },
    {
      "type": "header",
      "content": "Subtitle style"
    },
    {
      "type": "font_picker",
      "id": "subtitle_font",
      "label": "Font",
      "default": "helvetica_n4"
    },
    {
      "type": "range",
      "id": "subtitle_size",
      "min": 12,
      "max": 40,
      "step": 2,
      "unit": "px",
      "label": "Size (desktop)",
      "default": 18
    },
    {
      "type": "range",
      "id": "subtitle_size_mobile",
      "min": 12,
      "max": 30,
      "step": 2,
      "unit": "px",
      "label": "Size (mobile)",
      "default": 14
    },
    {
      "type": "color",
      "id": "subtitle_color",
      "label": "Color",
      "default": "#666666"
    },
    {
      "type": "header",
      "content": "Layout"
    },
    {
      "type": "range",
      "id": "max_width",
      "min": 600,
      "max": 1600,
      "step": 50,
      "unit": "px",
      "label": "Maximum width",
      "default": 1200
    },
    {
      "type": "range",
      "id": "columns_desktop",
      "min": 2,
      "max": 6,
      "step": 1,
      "label": "Columns (desktop)",
      "default": 4
    },
    {
      "type": "select",
      "id": "columns_mobile",
      "label": "Columns (mobile)",
      "options": [
        {
          "value": "1",
          "label": "1"
        },
        {
          "value": "2",
          "label": "2"
        }
      ],
      "default": "2"
    },
    {
      "type": "range",
      "id": "grid_gap",
      "min": 0,
      "max": 40,
      "step": 2,
      "unit": "px",
      "label": "Gap (desktop)",
      "default": 16
    },
    {
      "type": "range",
      "id": "grid_gap_mobile",
      "min": 0,
      "max": 30,
      "step": 2,
      "unit": "px",
      "label": "Gap (mobile)",
      "default": 8
    },
    {
      "type": "range",
      "id": "desktop_grid_side_gap",
      "min": 0,
      "max": 40,
      "step": 2,
      "unit": "px",
      "label": "Side gap (desktop)",
      "default": 0
    },
    {
      "type": "range",
      "id": "mobile_grid_side_gap",
      "min": 0,
      "max": 40,
      "step": 2,
      "unit": "px",
      "label": "Side gap (mobile)",
      "default": 8
    },
    {
      "type": "range",
      "id": "header_spacing",
      "min": 0,
      "max": 80,
      "step": 4,
      "unit": "px",
      "label": "Header bottom spacing",
      "default": 32
    },
    {
      "type": "range",
      "id": "grid_spacing",
      "min": 0,
      "max": 80,
      "step": 4,
      "unit": "px",
      "label": "Grid top spacing",
      "default": 0
    },
    {
      "type": "header",
      "content": "Item style"
    },
    {
      "type": "range",
      "id": "item_border_radius",
      "min": 0,
      "max": 40,
      "step": 2,
      "unit": "px",
      "label": "Border radius",
      "default": 8
    },
    {
      "type": "color",
      "id": "item_background",
      "label": "Background",
      "default": "#f5f5f5"
    },
    {
      "type": "color",
      "id": "placeholder_background",
      "label": "Placeholder background",
      "default": "#e0e0e0"
    },
    {
      "type": "header",
      "content": "Hover overlay"
    },
    {
      "type": "checkbox",
      "id": "show_overlay",
      "label": "Show overlay on hover",
      "default": true
    },
    {
      "type": "color",
      "id": "overlay_color",
      "label": "Overlay color",
      "default": "#000000"
    },
    {
      "type": "range",
      "id": "overlay_opacity",
      "min": 0,
      "max": 100,
      "step": 5,
      "unit": "%",
      "label": "Overlay opacity",
